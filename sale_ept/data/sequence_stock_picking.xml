<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_stock_picking" model="ir.sequence">
            <field name="name">Stock Picking</field>
            <field name="code">stock.picking</field>
            <field name="prefix">SP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_stock_picking_in" model="ir.sequence">
            <field name="name">Stock Picking</field>
            <field name="code">stock.picking.out</field>
            <field name="prefix">SP/Out/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_stock_picking_out" model="ir.sequence">
            <field name="name">Stock Picking</field>
            <field name="code">stock.picking.in</field>
            <field name="prefix">SP/In/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
